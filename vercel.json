{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/v1/health/check",
      "dest": "/server.js"
    },
    {
      "src": "/v1/projects/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/v1/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
