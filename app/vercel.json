{
    "version": 2,
    "builds": [
      {
        "src": "app/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/v1/health/check",
        "dest": "/app/server.js"
      },
      {
        "src": "/v1/projects/(.*)",
        "dest": "/app/server.js"
      },
      {
        "src": "/v1/(.*)",
        "dest": "/app/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/app/server.js"
      }
    ]
  }
  
  
  